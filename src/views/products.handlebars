<h1>Lista de Productos</h1>

<table>
    <tr>
        <th>Título</th>
        <th>Descripción</th>
        <th>Código</th>
        <th>Precio</th>
        <th>Estado</th>
        <th>Stock</th>
        <th>Categoría</th>
    </tr>
    {{#each products}}
    <tr>
        <td>{{this.title}}</td>
        <td>{{this.description}}</td>
        <td>{{this.code}}</td>
        <td>{{this.price}}</td>
        <td>{{this.status}}</td>
        <td>{{this.stock}}</td>
        <td>{{this.category}}</td>
    </tr>

    {{/each}}
</table>

<p>Hace click <a href="/products/pushProduct">acá</a> para añadir un nuevo producto</p>
